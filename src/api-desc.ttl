@prefix : <https://wetterdienst-rdf-adapter.deno.dev/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix hydra: <http://www.w3.org/ns/hydra/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .

<> a owl:Ontology ;
   dc:title "Wetterdienst RDF Adapter" ;
   owl:versionInfo "0.0.1" ;
   rdfs:seeAlso :root, :stations, :values .

:root a hydra:Resource ;
      http:requests :GET-root-200 ;
      rdfs:label "API Root" ;
      rdfs:comment "The root endpoint providing API status information." .

:stations a hydra:Resource ;
         http:requests :GET-stations-200, :GET-stations-400, :GET-stations-500 ;
         rdfs:label "Stations" ;
         rdfs:comment "The '/stations' endpoint for weather stations data." .

:values a hydra:Resource ;
       http:requests :GET-values-200, :GET-values-400, :GET-values-500 ;
       rdfs:label "Values" ;
       rdfs:comment "The '/values' endpoint for weather measurements data." .

# Root endpoint
:GET-root-200
    rdfs:label "GET '/' resource" ;
    http:methodName "GET" ;
    http:absolutePath "/" ;
    hydra:method "GET" ;
    http:resp [ 
        http:statusCodeNumber 200 ;
        http:reasonPhrase "OK" ;
        http:headers ( 
            [ http:fieldName "Content-Type" ; http:fieldValue "application/json" ] 
        )
    ] .

# Stations endpoint
:GET-stations-200
    rdfs:label "GET '/stations' resource" ;
    http:methodName "GET" ;
    http:absolutePath "/stations" ;
    hydra:method "GET" ;
    hydra:expects [
        a hydra:IriTemplate ;
        hydra:template "/stations{?provider,network,parameters,periods,coordinates,rank}" ;
        hydra:mapping [
            a hydra:IriTemplateMapping ;
            hydra:variable "provider" ;
            hydra:property schema:provider ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "network" ;
            hydra:property schema:network ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "parameters" ;
            hydra:property schema:parameters ;
            hydra:required "true"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "periods" ;
            hydra:property schema:periods ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "coordinates" ;
            hydra:property schema:coordinates ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "rank" ;
            hydra:property schema:rank ;
            hydra:required "false"^^xsd:boolean ;
        ]
    ] ;
    http:headers (
        [ http:fieldName "Accept" ; http:fieldValue "text/turtle, application/n-triples, application/n-quads, application/trig, text/n3, application/ld+json, application/json" ]
    ) ;
    http:resp [ 
        http:statusCodeNumber 200 ;
        http:reasonPhrase "OK" ;
        http:headers (
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "text/turtle" ;
              rdfs:comment "Default RDF format when Accept header requests RDF" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/n-triples" ;
              rdfs:comment "When Accept header is application/n-triples" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/n-quads" ;
              rdfs:comment "When Accept header is application/n-quads" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/trig" ;
              rdfs:comment "When Accept header is application/trig" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "text/n3" ;
              rdfs:comment "When Accept header is text/n3" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/ld+json" ;
              rdfs:comment "When Accept header is application/ld+json" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/json" ;
              rdfs:comment "Default format when no matching Accept header" ]
            [ http:fieldName "Vary" ; http:fieldValue "Accept" ]
        )
    ] .

:GET-stations-400
    rdfs:label "GET '/stations' error" ;
    http:methodName "GET" ;
    http:absolutePath "/stations" ;
    hydra:method "GET" ;
    http:resp [ 
        http:statusCodeNumber 400 ;
        http:reasonPhrase "Bad Request" ;
        http:headers ( [ http:fieldName "Content-Type" ; http:fieldValue "application/json" ] )
    ] .

:GET-stations-500
    rdfs:label "GET '/stations' server error" ;
    http:methodName "GET" ;
    http:absolutePath "/stations" ;
    hydra:method "GET" ;
    http:resp [ 
        http:statusCodeNumber 500 ;
        http:reasonPhrase "Internal Server Error" ;
        http:headers ( [ http:fieldName "Content-Type" ; http:fieldValue "application/json" ] )
    ] .

# Values endpoint
:GET-values-200
    rdfs:label "GET '/values' resource" ;
    http:methodName "GET" ;
    http:absolutePath "/values" ;
    hydra:method "GET" ;
    hydra:expects [
        a hydra:IriTemplate ;
        hydra:template "/values{?provider,network,parameters,periods,station,date}" ;
        hydra:mapping [
            a hydra:IriTemplateMapping ;
            hydra:variable "provider" ;
            hydra:property schema:provider ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "network" ;
            hydra:property schema:network ;
            hydra:required "false"^^xsd:boolean ;
                    ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "parameters" ;
            hydra:property schema:parameters ;
            hydra:required "true"^^xsd:boolean ;
            schema:valuePattern "[\\w_]+(/[\\w_]+)*" ;
            rdfs:comment "Hierarchical parameter path with components separated by '/'";
            schema:enumeration [
                a skos:ConceptScheme ;
                skos:hasTopConcept :param_1_minute, :param_5_minutes, :param_10_minutes,
                                :param_hourly, :param_subdaily, :param_daily,
                                :param_monthly, :param_annual ;
            ]
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "periods" ;
            hydra:property schema:periods ;
            hydra:required "false"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "station" ;
            hydra:property schema:station ;
            hydra:required "true"^^xsd:boolean ;
        ], [
            a hydra:IriTemplateMapping ;
            hydra:variable "date" ;
            hydra:property schema:date ;
            hydra:required "false"^^xsd:boolean ;
        ]
    ] ;
    http:headers (
        [ http:fieldName "Accept" ; http:fieldValue "text/turtle, application/n-triples, application/n-quads, application/trig, text/n3, application/ld+json, application/json" ]
    ) ;
    http:resp [ 
        http:statusCodeNumber 200 ;
        http:reasonPhrase "OK" ;
        http:headers (
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "text/turtle" ;
              rdfs:comment "Default RDF format when Accept header requests RDF" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/n-triples" ;
              rdfs:comment "When Accept header is application/n-triples" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/n-quads" ;
              rdfs:comment "When Accept header is application/n-quads" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/trig" ;
              rdfs:comment "When Accept header is application/trig" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "text/n3" ;
              rdfs:comment "When Accept header is text/n3" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/ld+json" ;
              rdfs:comment "When Accept header is application/ld+json" ]
            [ http:fieldName "Content-Type" ; 
              http:fieldValue "application/json" ;
              rdfs:comment "Default format when no matching Accept header" ]
            [ http:fieldName "Vary" ; http:fieldValue "Accept" ]
        )
    ] .

:GET-values-400
    rdfs:label "GET '/values' error" ;
    http:methodName "GET" ;
    http:absolutePath "/values" ;
    hydra:method "GET" ;
    http:resp [ 
        http:statusCodeNumber 400 ;
        http:reasonPhrase "Bad Request" ;
        http:headers ( [ http:fieldName "Content-Type" ; http:fieldValue "application/json" ] )
    ] .

:GET-values-500
    rdfs:label "GET '/values' server error" ;
    http:methodName "GET" ;
    http:absolutePath "/values" ;
    hydra:method "GET" ;
    http:resp [ 
        http:statusCodeNumber 500 ;
        http:reasonPhrase "Internal Server Error" ;
        http:headers ( [ http:fieldName "Content-Type" ; http:fieldValue "application/json" ] )
    ] .

